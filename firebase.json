{
  "hosting": {
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|png|svg|gif)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      }
    ],
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
}
}